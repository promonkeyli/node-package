{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "page": {
      "description": "create react page",
      "factory": "./page/index#page",
      "schema": "./page/schema.json",
      "aliases": ["p"]
    },
    "component": {
      "description": "create react component",
      "factory": "./component/index#component",
      "schema": "./component/schema.json",
      "aliases": ["c"]
    },
    "store": {
      "description": "create zustand store",
      "factory": "./store/index#store",
      "schema": "./store/schema.json",
      "aliases": ["s"]
    }
  }
}