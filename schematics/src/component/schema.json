{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "SchematicsReactComponent",
  "title": "React Component Options Schema",
  "type": "object",
  "description": "生成React模板代码文件",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "description": "组件名称",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "请输入你需要创建的组件名称"
    }
  },
  "required": ["name"]
}
